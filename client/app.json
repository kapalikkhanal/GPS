{
  "expo": {
    "name": "Stalker",
    "slug": "client",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icons/Stalker.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/splash-screen.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kapalikkhanal.client"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/Stalker.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/splash-screen.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0df51e58-d235-4263-852d-6692e858bc6c"
      }
    }
  }
}
